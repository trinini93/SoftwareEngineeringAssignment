<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.models.RegisFormEntity" table="RegisForm" schema="HelloWorld">
        <id name="idRegisForm">
            <column name="idRegisForm" sql-type="int(11)"/>
        </id>
        <property name="firstName">
            <column name="FirstName" sql-type="varchar(55)" length="55"/>
        </property>
        <property name="lastName">
            <column name="LastName" sql-type="varchar(55)" length="55"/>
        </property>
        <property name="address1">
            <column name="Address1" sql-type="varchar(55)" length="55"/>
        </property>
        <property name="address2">
            <column name="Address2" sql-type="varchar(55)" length="55"/>
        </property>
        <property name="city">
            <column name="City" sql-type="varchar(55)" length="55"/>
        </property>
        <property name="state">
            <column name="State" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="zipCode">
            <column name="ZipCode" sql-type="int(10)"/>
        </property>
        <property name="country">
            <column name="Country" sql-type="varchar(15)" length="15"/>
        </property>
        <property name="date">
            <column name="Date" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>